{
  "name": "@danielx/observable",
  "version": "0.5.0-pre.0",
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "browserify": "^17.0.0",
    "coffeescript": "^2",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  },
  "mocha": {
    "extension": [
      "coffee"
    ],
    "require": [
      "source-map-support",
      "ts-node/register",
      "coffeescript/register"
    ],
    "reporter": "spec",
    "recursive": true,
    "spec": [
      "test"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [],
    "sourceMap": true,
    "instrument": true
  },
  "scripts": {
    "prepare": "coffee --bare --compile --no-header main.coffee",
    "test": "nyc mocha"
  },
  "files": [
    "main.js"
  ],
  "main": "main.js",
  "description": "Fresh and intellegent observables",
  "repository": {
    "type": "git",
    "url": "git://github.com/DanielXMoore/Observable.git"
  },
  "keywords": [
    "observable"
  ],
  "author": "Daniel X. Moore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distri/observable/issues"
  },
  "homepage": "https://github.com/distri/observable"
}
